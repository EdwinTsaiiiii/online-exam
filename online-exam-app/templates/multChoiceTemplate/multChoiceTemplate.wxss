.multChoice-question {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin: 0 40rpx;
}

.multChoice-stem {
  padding: 20rpx 0;
  font-size: 35rpx;
}

.multoptions{
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding: 10rpx 0;
}

.multoption{
  display: flex;
  flex-direction: row;
  justify-content: left;
  padding: 10rpx 0;
  background-color: #eeeeee;
  padding: 20rpx;
  margin-top: 25rpx;
  border-radius: 20rpx;
}

.multoption_active{
  background-color: #e1f3d8;
}

.multcontent{
  font-size: 30rpx;
  line-height: 50rpx;
}